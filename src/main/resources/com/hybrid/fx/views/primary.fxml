<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hybrid.fx.controllers.PrimaryController">
   <top>
      <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="label" text="Welcome!" />
            <Button fx:id="button" mnemonicParsing="false" text="%button.text" />
            <TextField fx:id="url" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </top>
   <center>
      <WebView fx:id="myweb" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <Button fx:id="home" mnemonicParsing="false" text="Home" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
